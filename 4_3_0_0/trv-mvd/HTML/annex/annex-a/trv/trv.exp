TYPE IfcSlabTypeEnum = ENUMERATION OF
	(FLOOR
	,ROOF
	,LANDING
	,BASESLAB
	,APPROACH_SLAB
	,PAVING
	,WEARING
	,SIDEWALK
	,TRACKSLAB
	,USERDEFINED
	,NOTDEFINED);
